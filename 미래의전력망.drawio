        </mxCell>
        <mxCell id="pmCqmwbgw1cEMji928Z4-29" parent="pmCqmwbgw1cEMji928Z4-23" style="text;html=1;whiteSpace=wrap;strokeColor=none;fillColor=none;align=left;verticalAlign=middle;rounded=0;" value="필요한 이유" vertex="1">
